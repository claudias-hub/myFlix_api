{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["index.js", "auth.js", "passport.js", "models.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)(node_modules|movie_front|out|public)\\/|(^|\\/|\\\\)\\.git"
