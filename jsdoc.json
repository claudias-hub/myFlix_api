{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["index.js", "auth.js", "passport.js", "models.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)(node_modules|movie_front|docs|public)\\/|(^|\\/|\\\\)\\.git"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "tutorials": "./public",
    "readme": "./README.md"
    
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}